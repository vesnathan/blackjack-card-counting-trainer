{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "jquery": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "prettier/prettier": "error",
        "no-console": "warn"
    }
}
